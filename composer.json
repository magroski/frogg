{
    "name": "magroski/frogg",
    "description": "Nogg PHP Framework/Toolset for use with Phalcon Framework",
    "type": "library",
    "keywords": [
        "Phalcon",
        "Framework"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Lucas Magroski",
            "email": "magroski@gmail.com"
        },
        {
            "name": "Tasso Gama",
            "email": "tasso.gama@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Frogg\\": [
                "src"
            ]
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4",
        "aws/aws-sdk-php": "^3.27",
        "mobiledetect/mobiledetectlib": "^2.8",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "phalcon/ide-stubs": "^5.1",
        "phpstan/phpstan": "^1.9",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-doctrine": "^1.3",
        "phpstan/phpstan-strict-rules": "^1.4"
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse"
    }
}
